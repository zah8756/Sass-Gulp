{
  "name": "Sass-Gulp",
  "version": "1.1.0",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "description": "A Simple Example using React apps",
  "main": "./server/app.js",
  "scripts": {
    "buildExample1": "babel ./client/example1.js --presets react --out-file ./hosted/example1.js",
    "buildExample2": "babel ./client/example2.js --presets react --out-file ./hosted/example2.js",
    "buildExample3": "babel ./client/example3.js --presets react --out-file ./hosted/example3.js",
    "buildExample4": "babel ./client/example4.js --presets react --out-file ./hosted/example4.js",
    "buildExample5": "babel ./client/example5.js --presets react --out-file ./hosted/example5.js",
    "build": "npm run buildExample1 && npm run buildExample2 && npm run buildExample3 && npm run buildExample4 && npm run buildExample5",
    "watchExample1": "babel --watch ./client/example1.js --presets react --out-file ./hosted/example1.js",
    "watchExample2": "babel --watch ./client/example2.js --presets react --out-file ./hosted/example2.js",
    "watchExample3": "babel --watch ./client/example3.js --presets react --out-file ./hosted/example3.js",
    "watchExample4": "babel --watch ./client/example4.js --presets react --out-file ./hosted/example4.js",
    "watchExample5": "babel --watch ./client/example5.js --presets react --out-file ./hosted/example5.js",
    "buildSass": "node-sass --include-path scss ./scss/main.scss ./hosted/main.css",
    "watchSass": "node-sass --watch --include-path scss ./scss/main.scss ./hosted/main.css",
    "nodemon": "nodemon --watch ./server ./server/app.js ",
    "start": "node ./server/app.js",
    "pretest": "eslint ./server --fix",
    "test": "echo \"Tests complete\""
  },
  "author": "Cody Van De Mark / Austin Willoughby",
  "license": "Apache 2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.17.0",
    "mongoose": "^5.7.7",
    "serve-favicon": "^2.5.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4"
  }
}
